	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . --ext .ts,.svelte",
		"lint:fix": "eslint . --ext .ts,.svelte --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"build:github": "vite build",
		"deploy": "npm run build:github && npx gh-pages -d dist --nojekyll",
		"preprocess": "npx tsx scripts/preprocess-plants.ts",
		"preprocess:skip-wfo": "npx tsx scripts/preprocess-plants.ts --skip-wfo",
		"test:inaturalist": "npx tsx scripts/test-inaturalist.ts",
		"test:inaturalist:preprocessed": "npx tsx scripts/test-inaturalist.ts --preprocessed"
	},
